[gui]
CommandLine/HistoryFileItemSeparator=<EndItem>
CommandLine/HistoryFile=$HOME/.pdfedit/history
CommandLine/HistorySize=20
CommandLine/CmdShowEditor=true
CommandLine/CmdShowHistory=true
CommandLine/CmdShowLine=true
PageSpace/ResizingZone=2
items/MainMenu=list Main menu,file,tools,pagemenu,show,help
items/file=	list &File,fnew,fload,fsave,fsavecopy,fsaverev,-,neww,closew,-,quit
items/fnew=		item &New document,func_new(),,new.png
items/fload=		item &Open..., func_load(),F3,load.png
items/fsave=		item &Save,func_save(),F2,save.png
items/fsaverev=		item &Save new revision,func_saverev(),,save.png
items/fsavecopy=	item Save &copy...,func_savecopy(),,save.png
items/neww=		item New &Window,newwindow(),Ctrl+N,new_window.png
items/closew=		item &Close Window,closewindow,,close.png
items/quit=		item &Quit,quit
items/tools=	list &Tools,opts
items/opts=		item &Options,options(),F9,options.png
items/pagemenu=	list &Page,savepage,reloadpage,rot-90,rot90
items/reloadpage=	item Reload Page,go(),F5,reload.png
items/savepage=		item Save page as image...,savePageImage(false),,save_page_as_image.png
items/rot-90=		item Rotate page 270 degrees,rotatePage(-90),,page_rotate_-90.png
items/rot90=		item Rotate page 90 degrees,rotatePage(90),,page_rotate_90.png
items/show=	list &Show,hideCmd,hideProp,hideTree
items/hideCmd=		item Show/Hide commandline,showHide('commandline'),F8,hide_cmdline.png
items/hideProp=		item Show/Hide property editor,showHide('propertyeditor'),,hide_propedit.png
items/hideTree=		item Show/Hide object tree,showHide('tree'),,hide_tree.png
items/help=	list &Help,help_about,help_index
items/help_about=	item &About,about(),,about.png
items/help_index=	item &Help,help(),F1,help.png
items/MainToolbar=list Main Toolbar,fnew,fload,fsave,opts,closew,hideCmd
items/PageToolbar=list Page Toolbar,firstpage,prevpage,_page_tool,nextpage,lastpage,reloadpage,-,rot-90,rot90,-,zoom_out,_zoom_tool,zoom_in
items/firstpage=	item First Page,PageSpace.firstPage(),Ctrl+Home,first.png
items/prevpage=		item Prev Page,PageSpace.prevPage(),Ctrl+PgUp,prev.png
items/nextpage=		item Next Page,PageSpace.nextPage(),Ctrl+PgDown,next.png
items/lastpage=		item Last Page,PageSpace.lastPage(),Ctrl+End,last.png
items/zoom_in=		item Zoom in,PageSpace.zoomIn(),,zoom_in.png
items/zoom_out=		item Zoom out,PageSpace.zoomOut(),,zoom_out.png
items/RevisionToolbar=list Revision Toolbar,revlabel,_revision_tool
items/revlabel=		label Revisions
items/ObjectToolbar=list Object Toolbar,treeAdditem,treeDelitem
items/treeAdditem=	item Add object,add_obj_dlg(),Ins,item_add.png
items/treeDelitem=	item Delete object,treeitem.remove(),Del,item_delete.png
items/OperatorToolbar=list Operator Toolbar,streamAll,streamText,streamFont,-,operatorSetColor
items/streamAll=	item Show all operators in stream,streamMode('all'),,stream_mode_all.png
items/streamText=	item Show only text operators in stream,streamMode('text'),,stream_mode_text.png
items/streamFont=	item Show only font operators in stream,streamMode('font'),,stream_mode_font.png
items/operatorSetColor=	item Change color of operator,setColor(),,operator_setcolor.png
items/popup_generic=list Popup menu,tree_string,tree_type,tree_childs
items/tree_string=	item Show String representation,print(treeitem.item().getText())
items/tree_type=	item Show Item type,print(treeitem.itemtype())
items/tree_childs=	item Show Tree Childs,printTreeChilds(),F7,tree_childs.png
toolbars=MainToolbar,PageToolbar,ObjectToolbar,RevisionToolbar,OperatorToolbar
windowstate/main=796,608,10,2
windowstate/spl_left=369,172
windowstate/spl_main=517,269
